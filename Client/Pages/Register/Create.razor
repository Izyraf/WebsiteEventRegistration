@page "/registers/create/"
@inject HttpClient _client
@inject NavigationManager _navManager

<h3>class="card-title">Add Host</h3>
<hr />
<br />

<FormComponent ButtonText="Register Event" Register="Register" OnValidSubmit="@CreateRegister" />

@code {
    Register Register = new Register();

    private async Task CreateRegister()
    {
        await _client.PostAsJsonAsync(Endpoints.RegisterEndpoint, Register);
        _navManager.NavigateTo("/registers/");
    }
}